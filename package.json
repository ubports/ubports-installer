{
  "name": "ubports-installer",
  "version": "0.9.1-beta",
  "description": "The easy way to install Ubuntu Touch on UBports devices. A friendly cross-platform Installer for Ubuntu Touch. Just connect a supported device to your PC, follow the on-screen instructions and watch this awesome tool do all the rest.",
  "keywords": [
    "Ubuntu",
    "Ubuntu Touch",
    "UBports",
    "UBports Installer",
    "Android",
    "ADB",
    "Fastboot",
    "Heimdall"
  ],
  "homepage": "https://devices.ubuntu-touch.io",
  "bugs": {
    "url": "https://github.com/ubports/ubports-installer/issues",
    "email": "info@ubports.com"
  },
  "repository": "https://github.com/ubports/ubports-installer",
  "author": "Johannah Sprinz <neo@neothethird.de>",
  "license": "GPL-3.0",
  "main": "src/main.js",
  "bin": {
    "ubports-installer": "src/main.js"
  },
  "scripts": {
    "build": "npx rollup -c",
    "start": "./start.sh",
    "lint": "npx prettier \"**/!(*.min.*)\" --ignore-path=.gitignore --arrow-parens=avoid --trailing-comma=none -uw",
    "docs": "npx jsdoc -c jsdoc-config.json",
    "diagram": "npx arkit -f src/main.js",
    "test": "npx jest --coverage"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "arkit": "^1.6.4",
    "electron": "^15.4.1",
    "electron-builder": "^22.14.13",
    "electron-packager": "^15.4.0",
    "electron-reload": "^1.5.0",
    "jest": "^27.5.1",
    "jsdoc": "^3.6.10",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.6.0",
    "rollup": "^2.70.1",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.46.4",
    "svelte-spa-router": "^3.2.0"
  },
  "dependencies": {
    "7zip-min": "^1.4.2",
    "axios": "^0.26.1",
    "bootstrap": "^5.1.3",
    "cancelable-promise": "^4.3.0",
    "commander": "^8.3.0",
    "electron-open-link-in-browser": "^1.0.2",
    "electron-store": "^8.0.1",
    "form-data": "^4.0.0",
    "fs-extra": "^10.0.1",
    "graphql": "^15.8.0",
    "open-cuts-reporter": "^1.0.2",
    "progressive-downloader": "^1.0.7",
    "promise-android-tools": "^4.0.8",
    "ps-tree": "^1.2.0",
    "sudo-prompt": "^9.2.1",
    "svelte-markdown": "^0.2.2",
    "systeminformation": "^5.11.8",
    "tsparticles": "^1.42.2",
    "winston": "^3.6.0",
    "yaml": "^2.0.0-8"
  },
  "arkit": {
    "$schema": "https://arkit.pro/schema.json",
    "excludePatterns": [
      "__mocks__",
      "**/*.spec.*",
      "src/core/plugins/plugin.js"
    ],
    "components": [
      {
        "type": "Lib",
        "patterns": [
          "src/lib/**/*.js"
        ]
      },
      {
        "type": "Core",
        "patterns": [
          "src/core/**/*.js"
        ]
      },
      {
        "type": "Plugins",
        "patterns": [
          "src/core/plugins/**/*.js"
        ]
      },
      {
        "type": "Component",
        "patterns": [
          "src/**/*.js"
        ]
      }
    ],
    "output": [
      {
        "direction": "horizontal",
        "path": [
          "dist/arkit.svg",
          "dist/arkit.png"
        ],
        "groups": [
          {
            "first": true,
            "components": [
              "Component"
            ]
          },
          {
            "type": "Lib",
            "components": [
              "Lib"
            ]
          },
          {
            "type": "Core",
            "components": [
              "Core"
            ]
          },
          {
            "type": "Dependencies",
            "components": [
              "Dependency"
            ]
          }
        ]
      }
    ]
  }
}
