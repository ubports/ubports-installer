{
  "name": "ubports-installer",
  "version": "0.4.12-beta",
  "description": "The easy way to install Ubuntu Touch on UBports devices. A friendly cross-platform Installer for Ubuntu Touch. Just connect a supported device to your PC, follow the on-screen instructions and watch this awesome tool do all the rest.",
  "keywords": [
    "Ubuntu",
    "Ubuntu Touch",
    "UBports",
    "UBports Installer",
    "Android",
    "ADB",
    "Fastboot",
    "Heimdall"
  ],
  "homepage": "https://devices.ubuntu-touch.io",
  "bugs": {
    "url": "https://github.com/ubports/ubports-installer/issues",
    "email": "info@ubports.com"
  },
  "repository": "https://github.com/ubports/ubports-installer",
  "author": "UBports Foundation <info@ubports.com>",
  "license": "GPL-3.0",
  "main": "src/main.js",
  "bin": "./src/main.js",
  "scripts": {
    "start": "electron .",
    "dist:mac": "node ./build.js -o 'mac' -p 'dmg'",
    "dist:win": "node ./build.js -o \"win\" -p \"exe\"",
    "dist:linux": "node ./build.js -o 'linux' -p 'AppImage' && node ./build.js -o 'linux' -p 'deb' && snapcraft cleanbuild",
    "dist:linux:appimage": "node ./build.js -o 'linux' -p 'AppImage'",
    "dist:linux:deb": "node ./build.js -o 'linux' -p 'deb'",
    "dist:linux:snap": "snapcraft cleanbuild",
    "lint": "./node_modules/.bin/eslint *.js src/*.js src/html/scripts/*.js",
    "lint-fix": "./node_modules/.bin/eslint *.js src/*.js src/html/scripts/*.js --fix"
  },
  "devDependencies": {
    "electron": "^1.8.8",
    "electron-builder": "^20.36.2",
    "electron-packager": "^12.2.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "prettier": "^1.19.1",
    "sinon": "^7.5.0",
    "unzipper": "^0.9.4"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "checksum": "^0.1.1",
    "commander": "^2.9.0",
    "download": "^7.1.0",
    "electron-open-link-in-browser": "^1.0.2",
    "electron-prompt": "^1.4.0",
    "electron-pug": "^1.5.1",
    "fs-extra": "^2.0.0",
    "getos": "^3.0.1",
    "jquery": "^3.1.1",
    "jquery-i18next": "^1.2.0",
    "mkdirp": "^0.5.1",
    "popper.js": "^1.16.0",
    "promise-android-tools": "^2.0.1",
    "ps-tree": "^1.2.0",
    "request": "^2.79.0",
    "sudo-prompt": "^9.1.1",
    "system-image-node-module": "^1.0.10",
    "ubports-api-node-module": "^2.0.3",
    "winston": "^3.1.2"
  }
}
