{
  "name": "ubports-installer",
  "version": "0.5.2-beta",
  "description": "The easy way to install Ubuntu Touch on UBports devices. A friendly cross-platform Installer for Ubuntu Touch. Just connect a supported device to your PC, follow the on-screen instructions and watch this awesome tool do all the rest.",
  "keywords": [
    "Ubuntu",
    "Ubuntu Touch",
    "UBports",
    "UBports Installer",
    "Android",
    "ADB",
    "Fastboot",
    "Heimdall"
  ],
  "homepage": "https://devices.ubuntu-touch.io",
  "bugs": {
    "url": "https://github.com/ubports/ubports-installer/issues",
    "email": "info@ubports.com"
  },
  "repository": "https://github.com/ubports/ubports-installer",
  "author": "UBports Foundation <info@ubports.com>",
  "license": "GPL-3.0",
  "main": "src/main.js",
  "bin": "./src/main.js",
  "scripts": {
    "start": "npm run prerender && electron .",
    "prerender": "npx gulp pug",
    "dist:mac": "npm run prerender && node ./build.js -o 'mac' -p 'dmg'",
    "dist:win": "npm run prerender && node ./build.js -o \"win\" -p \"exe\"",
    "dist:linux": "npm run prerender && node ./build.js -o 'linux' -p 'AppImage' && node ./build.js -o 'linux' -p 'deb' && snapcraft snap",
    "dist:linux:appimage": "npm run prerender && node ./build.js -o 'linux' -p 'AppImage'",
    "dist:linux:deb": "npm run prerender && node ./build.js -o 'linux' -p 'deb'",
    "dist:linux:snap": "snapcraft snap",
    "lint": "npx eslint *.js \"src/**/*.js\"",
    "lint-fix": "npm run lint -- --fix"
  },
  "devDependencies": {
    "electron": "^10.1.3",
    "electron-builder": "^20.36.2",
    "electron-packager": "^15.1.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^1.19.1",
    "sinon": "^7.5.0",
    "gulp": "^4.0.2",
    "gulp-pug": "^4.0.1"
  },
  "dependencies": {
    "7zip-min": "^1.1.3",
    "axios": "^0.20.0",
    "bootstrap": "^3.3.7",
    "checksum": "^0.1.1",
    "commander": "^2.9.0",
    "electron-open-link-in-browser": "^1.0.2",
    "form-data": "^3.0.0",
    "fs-extra": "^2.0.0",
    "graphql": "^15.3.0",
    "graphql-request": "^3.1.0",
    "jquery": "^3.5.0",
    "jquery-i18next": "^1.2.0",
    "popper.js": "^1.16.0",
    "progressive-downloader": "^1.0.5",
    "promise-android-tools": "^2.1.0",
    "ps-tree": "^1.2.0",
    "sudo-prompt": "^9.2.1",
    "system-image-node-module": "^1.1.0",
    "systeminformation": "^4.27.7",
    "ubports-api-node-module": "^3.0.2",
    "winston": "^3.3.3"
  }
}
